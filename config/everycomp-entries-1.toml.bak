#Disables certain types. Note that all these configs, like in any other mod, only hide stuff from tabs and disable their recipes
[types]

		[types.wood_type.cobblemon]
			apricorn = true
			saccharine = true

		[types.wood_type.architects_palette]
			twisted = true

		[types.wood_type.mynethersdelight]
			powdery = true

		[types.wood_type.bountifulfares]
			hoary = true
			walnut = true

		[types.leaves_type.cobblemon]
			apricorn = true
			saccharine = true

		[types.leaves_type.architects_palette]
			twisted = true

		[types.leaves_type.bountifulfares]
			apple = true
			flowering_apple = true
			orange = true
			flowering_orange = true
			lemon = true
			flowering_lemon = true
			plum = true
			flowering_plum = true
			hoary = true
			walnut = true
			golden_apple = true
			flowering_golden_apple = true

		[types.cake.bountifulfares]
			coconut_cake = true
			cocoa_cake = true
			sponge_cake = true

#Disables specific entries
[entries]

		[entries.wood_type.farmersdelight]
			cabinet = true

		[entries.wood_type.create]
			window = true
			window_pane = true

		[entries.wood_type.architects_palette]
			board_stairs = true
			railing = true
			board_slab = true
			boards = true
			board_wall = true

		[entries.wood_type.unusual_furniture]
			railing = true
			celling_lamp = true
			carved = true
			stool = true
			bench = true
			drawer = true
			coffee_table = true
			beam = true
			table = true
			shelf = true
			open_riser_stairs = true
			chair = true

		[entries.wood_type.backpacked]
			backpack_shelf = true
